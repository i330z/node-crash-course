<!DOCTYPE html>
<html lang="en">
  <%- include("./partials/head.ejs") %>
  <body>
    <%- include("./partials/nav.ejs") %>

    <div class="blogs content">
      <h2>All Blogs</h2>
    </div>

    <% if(blogs.length > 0) { %> <% blogs.forEach(blog => { %>

    <h3 class="title"><%= blog.title %></h3>
    <p>
      <%= blog.body %>
    </p>

    <% }) %> <% } %>
  </body>
</html>
